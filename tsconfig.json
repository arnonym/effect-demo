{
  "compilerOptions": {
    "target": "es2016",
    "module": "es2022",
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
  }
}
